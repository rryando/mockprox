{
  "name": "@mockprox/cli",
  "description": "Mockprox's CLI. Deploy your mock APIs anywhere.",
  "version": "1.0.1",
  "author": {
    "name": "Ryan Rynaldo",
    "email": "rynaldo.dev@gmail.com",
    "url": "https://github.com/rryando"
  },
  "bin": {
    "mockprox-cli": "./bin/run.js"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint",
    "test": "glob -c \"node --import tsx --test-concurrency 1 --test\" \"./test/specs/**/*.test.ts\"",
    "build": "rm -rf dist && tsc -b tsconfig-build.json && oclif manifest",
    "start": "node ./bin/run.js start"
  },
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "/bin",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "commands": "./dist/commands",
    "hooks": {
      "init": "./dist/hooks/init"
    },
    "bin": "mockprox-cli",
    "dirname": "cli",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "https://github.com/rryando/mockprox",
  "homepage": "https://github.com/rryando/mockprox",
  "bugs": {
    "url": "https://github.com/rryando/mockprox/issues"
  },
  "dependencies": {
    "@inquirer/confirm": "5.0.0",
    "@mockprox/commons": "1.0.1",
    "@mockprox/commons-server": "1.0.1",
    "@oclif/core": "4.0.30",
    "@oclif/plugin-help": "6.2.16",
    "mkdirp": "3.0.1",
    "mustache": "4.2.0",
    "tslib": "2.8.0"
  },
  "devDependencies": {
    "@types/mustache": "4.2.5",
    "@types/node": "22.7.9",
    "eslint-config-oclif": "5.2.1",
    "eslint-config-oclif-typescript": "3.1.12",
    "oclif": "4.15.12",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": ">=18"
  }
}
